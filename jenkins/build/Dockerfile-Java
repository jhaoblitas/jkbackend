FROM openjdk:8

RUN mkdir /app

COPY *.jar  /app/app.jar

EXPOSE 8071

ENV BD_HOST="192.168.1.5"
ENV BD_DATABASE="BALOTARIO"
ENV BD_USERNAME="sa"
ENV BD_CLAVE="123456"

ENV OAUTH_CLIENTE="angularapp"
ENV OAUTH_SECRET="12345"
ENV TOKEN_TIMEOUT="9997200"

ENV PORT="8071"

CMD java -jar /app/app.jar  
